{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    "noEmit": false,
    "declaration": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ES5",
    "rootDir": "./example",
    "outDir": "./lib",
    "baseUrl": "./"
  },
  "include": ["example"],
  "exclude": ["node_modules"]
}
